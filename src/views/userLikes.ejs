<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('partials/head') %>
  </head>
  <body>
    <header><%- include('partials/header') %></header>
    <% if (users.length > 0) {%> <% users.forEach(user => { %>
    <h3><%= user.name %> <span><%= user.age %> </span></h3>
    <p><%= user.message %></p>
    <% }) %> <% }; %>
    <footer><%- include('partials/footer') %></footer>
  </body>
</html>
